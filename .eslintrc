{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {}
    }
  },
  "rules": {
    "no-console": 0,
    "prefer-destructuring": 0,
    "no-param-reassign": [2, { "props": false }]
  }
}
